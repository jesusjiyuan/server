<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">
  <class name="proope.business.to.entity.SalesOrderEntity" table="tbl_sales_order">
    <id name="dm" type="string">
        <column name="dm" />
        <generator class="assigned" />
    </id>
    <property name="khdm" type="string">
        <column name="khdm" />
    </property>
    <property name="zt" type="int">
        <column name="zt" />
    </property>
    <property name="bz" type="string">
        <column name="bz"  />
    </property>
	<property name="cjrq" type="timestamp">
	    <column name="cjrq" />
	</property>
	<property name="cjr" type="string">
        <column name="cjr" length="32" />
    </property>
	<property name="xgrq" type="timestamp">
	    <column name="xgrq" />
	</property>
	<property name="xgr" type="string">
        <column name="xgr" length="32" />
    </property>
  </class>
</hibernate-mapping>