<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">
  <class name="proope.purchase.to.entity.PurchaseOrderDetailEntity" table="tbl_purchase_order_detail">
    <id name="dm" type="string">
        <column name="dm" />
        <generator class="assigned" />
    </id>
    <property name="cgdddm" type="string">
        <column name="cgdddm" />
    </property>
    <property name="cgsqmxdm" type="string">
        <column name="cgsqmxdm" />
    </property>
    <property name="wldm" type="string">
        <column name="wldm" />
    </property>
    <property name="dj" type="java.math.BigDecimal">
        <column name="dj" />
    </property>
    <property name="sqsl" type="java.math.BigDecimal">
        <column name="sqsl" />
    </property>
    <property name="dgsl" type="java.math.BigDecimal">
        <column name="dgsl" />
    </property>
    <property name="ydhsl" type="java.math.BigDecimal">
        <column name="ydhsl" />
    </property>
    <property name="bz" type="string">
        <column name="bz" />
    </property>
	<property name="cjrq" type="timestamp">
	    <column name="cjrq" />
	</property>
	<property name="cjr" type="string">
        <column name="cjr" length="32" />
    </property>
  </class>
</hibernate-mapping>